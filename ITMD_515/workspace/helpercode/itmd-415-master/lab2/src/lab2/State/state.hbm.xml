<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="lab2.State.State" table="state">
  <id column="comment_id" name="state_id" type="java.lang.Integer"/>
  <property column="name" generated="never" lazy="false" name="name" type="java.lang.String"/>
  <property column="content" generated="never" lazy="false"
   name="abbreviation" type="java.lang.String"/>
 </class>
</hibernate-mapping>
